{
  "name": "Rate My Professor Lookup",
  "description": "Looks up a Professor's RMP profile with one click",
  "version": "1.3",
  "manifest_version": 3,
  "permissions": ["contextMenus", "tabs"],
  "background": {
    "service_worker": "lookup.js",
    "type": "module"
  },
"action": {
    "default_popup": "popup.html"
  },

  "icons": {
   "16": "rmp16.png",
   "48": "rmp48.png",
  "128": "rmp128.png"
 }
}
